<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="bitcamp.app.dao.GeneratedImgDao">

  <resultMap type="GeneratedImg" id="GeneratedImgMap">
    <id column="img_no" property="imgNo"/>
    <result column="img_filename" property="filename"/>
    <result column="board_no" property="boardNo"/>
  </resultMap>

  <insert id="insert" parameterType="GeneratedImg">
    insert into aim_generated_img(img_filename, board_no)
      values(#{filename}, #{boardNo})
  </insert>
  
</mapper>